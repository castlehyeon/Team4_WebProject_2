<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.kosa.scheduleManagement.global.dao.ScheduleBoard_Project_EmpDao">

	<select id="getAllProjectEmpList"
		resultType="com.kosa.scheduleManagement.global.dto.Project_Emp">
		select user_id, project_num
		from project_emp
	</select>
	
		<select id="getAllProjectEmpListByProject"
		resultType="com.kosa.scheduleManagement.global.dto.Project_Emp">
		select user_id, project_num
		from project_emp
		where project_num=2
	</select>

</mapper>
	